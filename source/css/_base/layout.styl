*
  margin 0
  padding 0

.logo-exterior
  height 90px
  width 1000px
  position: relative;
  display: inline-block
  //box-shadow: 0 0 16px rgba(0,0,0,0.3)
  border-radius: 2px

.logo-container
  background url('images/logo-small.png')
  //margin 0 auto
  height 90px
  width 276px
  float left


.round
  box-shadow: 0 0 16px rgba(0,0,0,0.3)
  border-radius: 25px
  border: 2px solid #ffffff
  padding: 10px

body
  background color-background
  background-attachment fixed
  background-image url('images/bg.png')
  font 300 14px font-main
  text-shadow 0 0 1px transparent
  z-index -1

h1
  font-size 2em

h2
  font-size 1.6em

h3
  font-size 1.3em

a
  color color-main01
  text-decoration none

strong
  font-weight bold

.alignleft
  float left

.alignright
  float right

.clearfix
  clearfix()

.inner
  margin 0 auto
  //position relative
  width 1000px
  z-index 2