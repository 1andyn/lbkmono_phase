<% if (item.layout == 'page') { %>
    <script>
        var fileName = location.href.split("/").slice(-1).toString();
        var id = "nav-" + fileName.slice(0,-5); // 5 Chars = ".html"
        document.getElementById(id).className = "round";

        /* Secondary Logic, check if Menu item Exists*/
        var sec = document.getElementById(id + "-menu");
        if(sec != null) {
            sec.className = "round";
        }
    </script>
<% } %>

<article class="<% if (item.layout == 'page'){ %>page<% } else { %>post<% } %>">
  <% if (item.photos){ %>
    <%- partial('post/gallery') %>
  <% } %>
  <header>
    <%- partial('post/title') %>
    <% if (item.layout != 'page'){ %><%- partial('post/date') %><% } %>
  </header>
  <div class="entry-content">
    <% if (item.excerpt && index){ %>
      <%- item.excerpt %>
    <% } else { %>
      <%- item.content %>
    <% } %>
    <% if (item.layout != 'page'){ %>
    <footer class="meta">
      <%- partial('post/category') %>
      <%- partial('post/tag') %>
      <% if (config.disqus_shortname && item.comment){ %>
        <a href="<%- item.permalink %>#disqus_thread" class="comment"><%= __('comment') %></a>
      <% } %>
    </footer>
      <%- partial('comment') %>
    <% } %>
  </div>
</article>